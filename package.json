{
  "name": "react-ui",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/libs/*",
    "packages/ui/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "lint:foreach": "turbo run lint",
    "format": "prettier --write",
    "lint": "eslint",
    "format:foreach": "prettier --write \"**/*.{ts,tsx,md}\"",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@3.5.0"
}
